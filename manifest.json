{
  "manifest_version": 3,
  "name": "Placeholder to Value",
  "version": "1.0.0",
  "description": "Double-click empty text fields to fill them with their placeholder text",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
